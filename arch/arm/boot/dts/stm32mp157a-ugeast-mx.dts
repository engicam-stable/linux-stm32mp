&fmc {
	pinctrl-names = "default";
	pinctrl-0 = <&fmc_pins_a>;
	status = "okay";
	#address-cells = <1>;
	#size-cells = <0>;

	nand: nand@0 {
		reg = <0>;
		nand-on-flash-bbt;
		#address-cells = <1>;
		#size-cells = <1>;
		
        partition@0 {
            label = "nand-boot";
            reg = <0x00000000 0x01400000>;
        };

        partition@1400000 {
            label = "nand-fs";
            reg = <0x01400000 0x1e000000>;
        };
	};
};


&adc {
