&ethernet0 {
    pinctrl-names = "default";
    pinctrl-0 = <&eth1_pins_mx  &eth_res>;
	phy-mode = "rmii";
	max-speed = <100>;
	phy-handle = <&phy0>;
	snps,ps-speed = <100>;
    st,eth_ref_clk_sel;
	status = "okay";

    clock-names = "stmmaceth",
                "mac-clk-tx",
                "mac-clk-rx",
                "ethstp",
                "syscfg-clk",
                "eth-ck";

    clocks = <&rcc ETHMAC>,
            <&rcc ETHTX>,
            <&rcc ETHRX>,
            <&rcc ETHSTP>,
            <&rcc SYSCFG>,
            <&rcc ETHCK_K>;
	
    mdio0 {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "snps,dwmac-mdio";
		snps,reset-gpio = <&gpioa 13 0>;
		snps,reset-delays-us = <25>;
		snps,reset-active-low;

		phy0: ethernet-phy-0 {
			reg = <0>;
		};
	};
};

