                     <STM32_PINMUX('Z', 6, AF2)>; /* I2C6_SCL */
            bias-disable;
            drive-open-drain;
            slew-rate = <0>;
        };
    };
    i2c6_sleep_pins_z_mx: i2c6_sleep_mx-0 {
        pins {
            pinmux = <STM32_PINMUX('Z', 7, ANALOG)>, /* I2C6_SDA */
                     <STM32_PINMUX('Z', 6, ANALOG)>; /* I2C6_SCL */
        };
    };
};

